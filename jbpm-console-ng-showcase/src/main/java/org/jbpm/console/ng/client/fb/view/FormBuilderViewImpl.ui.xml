<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
             xmlns:c="urn:import:com.google.gwt.user.cellview.client">

    <ui:with field="i18n" type="org.jbpm.console.ng.client.i18n.Constants"/>
    <ui:with field='resources' type='org.jbpm.console.ng.client.resources.ShowcaseResources'/>
    
 
    <gwt:DockPanel>
        <gwt:Dock direction="WEST" size="230px">   
            <gwt:FlowPanel>
                <gwt:ScrollPanel  ui:field="menuView">
                
                </gwt:ScrollPanel>
             
                <gwt:Button ui:field="saveButton">Save</gwt:Button>
                <gwt:Button ui:field="clearButton">Clear</gwt:Button>
            </gwt:FlowPanel>
        </gwt:Dock>
        <gwt:Dock direction="SOUTH" size="760px">
            
            <gwt:ScrollPanel  ui:field="layoutView">
                
            </gwt:ScrollPanel>
        </gwt:Dock>
        
        
        
        
    </gwt:DockPanel>
      
</ui:UiBinder>